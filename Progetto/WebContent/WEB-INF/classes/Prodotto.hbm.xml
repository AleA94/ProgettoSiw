<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="DAO.Prodotto" table="Prodotto">
      <id name="idProdotto" type="int" column="idProdotto">
      </id>
      <property name="nome" column="Nome" type="string"/>
      <property name="descrizione" column="Descrizione" type="string"/>
      <property name="inAsta" column="inAsta" type="int"/>
      <property name="Prezzo" column="Prezzo" type="float"/>
      <property name="idCategoria" column="idCategoria" type="int"/>
      <property name="dataInizio" column="DataInizio" type="date"/>
      <property name="dataFine" column="DataFine" type="date"/>
   </class>
</hibernate-mapping>